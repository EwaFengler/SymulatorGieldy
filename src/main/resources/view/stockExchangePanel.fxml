<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane fx:id="stockExchangePanel" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controller.StockExchangePanelController">
   <GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="150.0"
             prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
             AnchorPane.topAnchor="0.0">
      <columnConstraints>
         <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
         <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
         <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
         <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
         <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
         <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
         <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
      </columnConstraints>
      <rowConstraints>
         <RowConstraints maxHeight="76.0" minHeight="10.0" prefHeight="31.0" vgrow="SOMETIMES"/>
         <RowConstraints maxHeight="259.0" minHeight="10.0" prefHeight="39.0" valignment="BOTTOM" vgrow="SOMETIMES"/>
         <RowConstraints maxHeight="128.0" minHeight="7.0" prefHeight="89.0" valignment="TOP" vgrow="SOMETIMES"/>
      </rowConstraints>
      <Button fx:id="createStockExchangeButton" mnemonicParsing="false" onAction="#createStockExchange" text="Dodaj"
              GridPane.columnIndex="6" GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
         </GridPane.margin>
      </Button>
      <Label text="Dodaj giełdę" GridPane.valignment="TOP">
         <GridPane.margin>
            <Insets left="5.0" top="10.0"/>
         </GridPane.margin>
      </Label>
      <Label text="Nazwa" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets bottom="5.0" left="5.0"/>
         </GridPane.margin>
      </Label>
      <Label text="Kraj" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets bottom="5.0" left="5.0"/>
         </GridPane.margin>
      </Label>
      <Label text="Waluta" GridPane.columnIndex="2" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets bottom="5.0" left="5.0"/>
         </GridPane.margin>
      </Label>
      <Label text="Miasto" GridPane.columnIndex="3" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets bottom="5.0" left="5.0"/>
         </GridPane.margin>
      </Label>
      <Label text="Adres siedziby" GridPane.columnIndex="4" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets bottom="5.0" left="5.0"/>
         </GridPane.margin>
      </Label>
      <Label text="Marża procentowa" GridPane.columnIndex="5" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets bottom="5.0" left="5.0"/>
         </GridPane.margin>
      </Label>
      <ComboBox fx:id="currency" prefWidth="150.0" promptText="Wybierz walutę" GridPane.columnIndex="2"
                GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
         </GridPane.margin>
      </ComboBox>
      <TextField fx:id="name" GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
         </GridPane.margin>
      </TextField>
      <TextField fx:id="city" GridPane.columnIndex="3" GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
         </GridPane.margin>
      </TextField>
      <TextField fx:id="percentageMarkup" GridPane.columnIndex="5" GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
         </GridPane.margin>
      </TextField>
      <TextArea fx:id="address" GridPane.columnIndex="4" GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets bottom="20.0" left="5.0" right="5.0" top="5.0"/>
         </GridPane.margin>
      </TextArea>
      <ComboBox fx:id="country" prefWidth="150.0" promptText="Wybierz kraj" GridPane.columnIndex="1"
                GridPane.rowIndex="2">
         <opaqueInsets>
            <Insets/>
         </opaqueInsets>
         <GridPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
         </GridPane.margin>
      </ComboBox>
   </GridPane>
</AnchorPane>
